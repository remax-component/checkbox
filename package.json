    "prettier": "^2.0.4",
    "puppeteer": "^3.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "remax": "^2.0.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "clsx": "^1.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,less,md}": "prettier --write",
    "*.{ts,tsx}": "eslint --cache --fix"
  }
